{
  "manifest_version": 3,
  "name": "Oompa-Loompa Swap",
  "version": "1.0.0",
  "description": "Replaces AI terms on LinkedIn with Oompa-Loompas.",
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
